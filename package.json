{
  "name": "spectator-be",
  "version": "1.0.0",
  "description": "This is a backend for final year spectator project",
  "main": "server.js",
  "scripts": {
    "build": "babel src -d dist",
    "dev": "cross-env NODE_ENV=development nodemon --exec npx babel-node src/server.js",
    "start": "cross-env NODE_ENV=production npm run unmigrate && npm run migrate && npm run build && node dist/server.js",
    "migrate": "npx sequelize-cli db:migrate",
    "unmigrate": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed-roles": "npx sequelize-cli db:seed --seed 20221017145146-seed-role",
    "unseed": "npx sequelize-cli db:seed:undo:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Three-Aces/spectator-be.git"
  },
  "author": "three-aces",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Three-Aces/spectator-be/issues"
  },
  "homepage": "https://github.com/Three-Aces/spectator-be#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.32.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "joi": "^17.6.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "pdf-puppeteer": "^1.1.10",
    "pdfkit": "^0.13.0",
    "sequelize": "^6.23.1",
    "socket.io": "^4.5.3",
    "swagger": "^0.7.5",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/node": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize-cli": "^6.5.1"
  }
}
